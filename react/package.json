{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "homepage": "app-root/template",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "moment-timezone": "^0.5.43",
    "odata-query": "^7.0.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.7",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:build && npm run build:rmmap && npm run build:rename && npm run build:zip && npm run build:unrename",
    "build:build": "react-scripts build",
    "build:rmmap": "rm ./build/static/js/*.map",
    "build:rename": "mv build template && touch template.app",
    "build:zip": "rm template.app && zip -r9v template.app template",
    "build:unrename": "mv template build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "sass": "^1.63.6"
  },
  "proxy": "http://192.168.0.8:9000/rest"
}
