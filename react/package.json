{
  "name": "template",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "type": "module",
  "dependencies": {
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "jotai": "^2.16.0",
    "moment-timezone": "^0.6.0",
    "odata-query": "^8.0.7",
    "react": "^19.2.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.1",
    "react-router-dom": "^7.10.1",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "rsbuild dev",
    "clean": "rm -Rf node_modules && rm *.app && rm -Rf build",
    "build": "npm run build:build && npm run build:fix-css && npm run build:rename && npm run build:zip && npm run build:unrename",
    "build:build": "NODE_ENV=production rsbuild build",
	"build:fix-css": "sed -i '' 's|url(./static|url(../../static|g' build/static/css/*.css",
    "build:rmmap": "rm ./build/static/js/*.map",
    "build:rename": "mv build template && touch template.app",
    "build:zip": "rm template.app && zip -r9v template.app template",
    "build:unrename": "mv template build",
	"preview": "rsbuild preview",
	"test": "rsbuild test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.14",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "sass": "^1.95.1",
    "typescript": "^5.9.3"
  },
  "proxy": "http://localhost:9000/rest"
}
